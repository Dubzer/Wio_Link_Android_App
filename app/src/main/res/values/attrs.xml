<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SlidingTabStrip">
        <attr name="psIndicatorColor" format="color" />
        <attr name="psUnderlineColor" format="color" />
        <attr name="psDividerColor" format="color" />
        <attr name="psIndicatorHeight" format="dimension" />
        <attr name="psUnderlineHeight" format="dimension" />
        <attr name="psDividerPadding" format="dimension" />
        <attr name="psTabPaddingLeftRight" format="dimension" />
        <attr name="psScrollOffset" format="dimension" />
        <attr name="psTabBackground" format="reference" />
        <attr name="psShouldExpand" format="boolean" />
        <attr name="psTextAllCaps" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CusFontTextView">
        <attr name="cusTextFont" format="string" />
    </declare-styleable>
    <declare-styleable name="CusFontEditView">
        <attr name="cusEditFont" format="string" />
    </declare-styleable>

</resources>